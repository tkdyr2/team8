create table `Use`(
Use_ID_P int primary key,
Start_time time,
Finish_time time,
User_number_F varchar(12),
Locaton_information_F int,
foreign key(User_number_F) references USER(Customer_Number_P),
foreign key(Locaton_information_F) references Washshing_machine_Location_information(Location_ID_P));
